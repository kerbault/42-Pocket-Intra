{
  "name": "42 Pocket Intra",
  "options_page": "options.html",
  "version": "0.1",
  "description": "Portable 42Intra extension",
  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ],
  "background": {
    "scripts": [
      "logTime.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "pictures/42_logo16.png",
      "32": "pictures/42_logo32.png",
      "48": "pictures/42_logo48.png",
      "128": "pictures/42_logo128.png"
    }
  },
  "icons": {
    "16": "pictures/42_logo16.png",
    "32": "pictures/42_logo32.png",
    "48": "pictures/42_logo48.png",
    "128": "pictures/42_logo128.png"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}